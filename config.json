{
    "dataset_reader": {
        "type": "udify_universal_dependencies",
        "lazy": false,
        "token_indexers": {
            "bert": {
                "type": "udify-bert-pretrained",
                "do_lowercase": false,
                "pretrained_model": "config/archive/bert-base-multilingual-cased/vocab.txt",
                "use_starting_offsets": true
            },
            "tokens": {
                "type": "single_id",
                "lowercase_tokens": true
            }
        }
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 16,
        "biggest_batch_first": true,
        "maximum_samples_per_batch": [
            "num_tokens",
            1600
        ],
        "sorting_keys": [
            [
                "tokens",
                "num_tokens"
            ]
        ]
    },
    "model": {
        "type": "udify_model",
        "decoders": {
            "deps": {
                "type": "udify_dependency_decoder",
                "arc_representation_dim": 768,
                "dropout": 0.5,
                "encoder": {
                    "type": "pass_through",
                    "input_dim": 768
                },
                "pos_embed_dim": null,
                "tag_representation_dim": 256
            },
            "feats": {
                "type": "udify_tag_decoder",
                "adaptive": true,
                "dropout": 0.5,
                "encoder": {
                    "type": "pass_through",
                    "input_dim": 768
                },
                "label_smoothing": 0.03,
                "task": "feats"
            },
            "lemmas": {
                "type": "udify_tag_decoder",
                "adaptive": true,
                "dropout": 0.5,
                "encoder": {
                    "type": "pass_through",
                    "input_dim": 768
                },
                "label_smoothing": 0.03,
                "task": "lemmas"
            },
            "upos": {
                "type": "udify_tag_decoder",
                "dropout": 0.5,
                "encoder": {
                    "type": "pass_through",
                    "input_dim": 768
                },
                "label_smoothing": 0.03,
                "task": "upos"
            }
        },
        "dropout": 0.5,
        "encoder": {
            "type": "pass_through",
            "input_dim": 768
        },
        "layer_dropout": 0.1,
        "mix_embedding": 12,
        "tasks": [
            "upos",
            "feats",
            "lemmas",
            "deps"
        ],
        "text_field_embedder": {
            "type": "udify_embedder",
            "allow_unmatched_keys": true,
            "dropout": 0.5,
            "embedder_to_indexer_map": {
                "bert": [
                    "bert",
                    "bert-offsets"
                ]
            },
            "token_embedders": {
                "bert": {
                    "type": "udify-bert-pretrained",
                    "combine_layers": "all",
                    "dropout": 0.15,
                    "layer_dropout": 0.1,
                    "pretrained_model": "bert-base-multilingual-cased",
                    "requires_grad": true
                }
            }
        },
        "word_dropout": 0.2
    },
    "train_data_path": "data/ud-treebanks-v2.3/UD_English-EWT/en_ewt-ud-train.conllu",
    "validation_data_path": "data/ud-treebanks-v2.3/UD_English-EWT/en_ewt-ud-dev.conllu",
    "test_data_path": "data/ud-treebanks-v2.3/UD_English-EWT/en_ewt-ud-test.conllu",
    "trainer": {
        "cuda_device": 0,
        "grad_clipping": 10,
        "grad_norm": 5,
        "learning_rate_scheduler": {
            "type": "ulmfit_sqrt",
            "decay_factor": 0.04,
            "discriminative_fine_tuning": true,
            "factor": 5,
            "gradual_unfreezing": true,
            "model_size": 1,
            "start_step": 392,
            "warmup_steps": 392
        },
        "num_epochs": 80,
        "num_serialized_models_to_keep": 1,
        "optimizer": {
            "type": "bert_adam",
            "b1": 0.9,
            "b2": 0.99,
            "lr": 0.001,
            "parameter_groups": [
                [
                    [
                        "^text_field_embedder.*.bert_model.embeddings",
                        "^text_field_embedder.*.bert_model.encoder"
                    ],
                    {}
                ],
                [
                    [
                        "^text_field_embedder.*._scalar_mix",
                        "^text_field_embedder.*.pooler",
                        "^scalar_mix",
                        "^decoders",
                        "^shared_encoder"
                    ],
                    {}
                ]
            ],
            "weight_decay": 0.01
        },
        "patience": 40,
        "should_log_learning_rate": true,
        "should_log_parameter_statistics": false,
        "summary_interval": 100,
        "validation_metric": "+.run/.sum"
    },
    "vocabulary": {
        "directory_path": "data/vocab/english_only_expmix/vocabulary"
    },
    "evaluate_on_test": true
}